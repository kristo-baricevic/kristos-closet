#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kristo/kristos-closet/client/closet-app/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.22.5_@babel+plugin-transform-react-jsx@7.22.5_adjxsrvzeevl7eyzj4f67l2fvq/node_modules/react-scripts/bin/node_modules:/Users/kristo/kristos-closet/client/closet-app/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.22.5_@babel+plugin-transform-react-jsx@7.22.5_adjxsrvzeevl7eyzj4f67l2fvq/node_modules/react-scripts/node_modules:/Users/kristo/kristos-closet/client/closet-app/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.22.5_@babel+plugin-transform-react-jsx@7.22.5_adjxsrvzeevl7eyzj4f67l2fvq/node_modules:/Users/kristo/kristos-closet/client/closet-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/kristo/kristos-closet/client/closet-app/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.22.5_@babel+plugin-transform-react-jsx@7.22.5_adjxsrvzeevl7eyzj4f67l2fvq/node_modules/react-scripts/bin/node_modules:/Users/kristo/kristos-closet/client/closet-app/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.22.5_@babel+plugin-transform-react-jsx@7.22.5_adjxsrvzeevl7eyzj4f67l2fvq/node_modules/react-scripts/node_modules:/Users/kristo/kristos-closet/client/closet-app/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.22.5_@babel+plugin-transform-react-jsx@7.22.5_adjxsrvzeevl7eyzj4f67l2fvq/node_modules:/Users/kristo/kristos-closet/client/closet-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../react-scripts/bin/react-scripts.js" "$@"
else
  exec node  "$basedir/../react-scripts/bin/react-scripts.js" "$@"
fi
