#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/kristo/kristos-closet/client/closet-app/node_modules/html-minifier-terser/node_modules:/Users/kristo/kristos-closet/client/closet-app/node_modules:/Users/kristo/kristos-closet/client/node_modules:/Users/kristo/kristos-closet/node_modules:/Users/kristo/node_modules:/Users/node_modules:/node_modules:/Users/kristo/kristos-closet/client/closet-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/kristo/kristos-closet/client/closet-app/node_modules/html-minifier-terser/node_modules:/Users/kristo/kristos-closet/client/closet-app/node_modules:/Users/kristo/kristos-closet/client/node_modules:/Users/kristo/kristos-closet/node_modules:/Users/kristo/node_modules:/Users/node_modules:/node_modules:/Users/kristo/kristos-closet/client/closet-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../html-minifier-terser/cli.js" "$@"
else
  exec node  "$basedir/../html-minifier-terser/cli.js" "$@"
fi
