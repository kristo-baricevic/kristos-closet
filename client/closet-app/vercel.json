{
    "routes": [
        { 
            "src": "api/images/(.*)", 
            "dest": "routes.js" ,
            "methods": ["GET", "PUT", "DELETE"]
        },
        {
             "src": "api/upload/(.*)", 
             "dest": "routes.js",
             "methods": ["POST"] 
        },
        { 
            "src": "api/register/(.*)", 
            "dest": "routes.js",
            "methods": ["POST"]
        },
        { 
            "src": "api/login/(.*)",
            "dest": "routes.js",
            "methods": ["POST"]
         },
        { 
            "src": "api/logout/(.*)", 
            "dest": "routes.js",
            "methods": ["POST"]
        },
        { 
            "src": "api/loginAnonymous/(.*)", 
            "dest": "routes.js", 
            "methods": ["POST"] 
        },
        { 
            "src": "api/current/(.*)", 
            "dest": "routes.js",
            "methods": ["GET"]
        },
        { 
            "src": "/static/(.*)", 
            "dest": "static/$1" 
        },
        { 
            "src": "/(.*)",
            "dest": "index.html" 
        }
    ]
}
  